{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport moment from \"moment\";\nimport TeacherRepository from \"@/repositories/TeacherRepository\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\nexport default {\n  name: \"ActiveAssignment\",\n  data() {\n    return {\n      assignments: []\n    };\n  },\n  methods: {\n    getAssignments: function () {\n      StudentRepository.getAssignments(1).then(response => {\n        const data = response.data.data;\n        for (let i = 0; i < data.length; i++) {\n          let item = data[i];\n          let assignmentDetail = {\n            assignmentID: item.id,\n            subjectName: item.subject_name,\n            classroomName: item.class_name,\n            title: item.title,\n            description: item.written_description,\n            dueDatetime: item.due_datetime\n          };\n          this.assignments.push(assignmentDetail);\n        }\n      });\n    },\n    getHumanDate(datetime) {\n      return moment(datetime, \"YYYY-MM-DD hh:mm:ss\").format(\"DD MMMM YYYY\");\n    }\n  },\n  mounted() {\n    this.getAssignments();\n  }\n};","map":{"version":3,"names":["moment","TeacherRepository","StudentRepository","name","data","assignments","methods","getAssignments","then","response","i","length","item","assignmentDetail","assignmentID","id","subjectName","subject_name","classroomName","class_name","title","description","written_description","dueDatetime","due_datetime","push","getHumanDate","datetime","format","mounted"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/students/StudentHome/Components/ActiveAssignment.vue"],"sourcesContent":["<template>\n  <div>\n    <router-link :to=\"{name: 'student.assignments.show', params: { assignmentID: item.assignmentID }}\" :key=\"item.assignmentID\" v-for=\"item in assignments\" class=\"max-w-sm h-30 rounded rounded-xl justify-between overflow-hidden bg-gray-secondary flex flex-col px-3 pt-5 pb-3 mb-2\">\n      <div class=\"text-left text-purple-primary text-xs-plus truncate  pr-10\">{{ item.title }}</div>\n\n      <div class=\"text-left text-purple-primary text-px-10 truncate pr-10\">{{ item.description }}</div>\n\n      <div class=\"flex flex-row\">\n\n        <div class=\"flex flex-row w-3/4 text-left text-px-10 text-purple-secondary\">\n          <div class=\"w-1/4 truncate\"> {{ item.subjectName }}</div>\n          <div class=\"w-1/4  truncate mx-1\"> {{ item.classroomName }}</div>\n          <div class=\"w-2/4\"> {{ getHumanDate(item.dueDatetime) }}</div>\n        </div>\n\n        <div class=\"w-1/4 bg-purple-secondary rounded-full\">\n\n        </div>\n      </div>\n\n    </router-link>\n  </div>\n\n</template>\n\n<script>\nimport moment from \"moment\";\nimport TeacherRepository from \"@/repositories/TeacherRepository\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\n\nexport default {\n  name: \"ActiveAssignment\",\n  data() {\n    return {\n      assignments : []\n    }\n  },\n  methods: {\n    getAssignments: function () {\n\n      StudentRepository.getAssignments(1)\n          .then(response => {\n\n            const data = response.data.data\n\n            for (let i = 0; i < data.length; i++) {\n\n              let item = data[i];\n\n              let assignmentDetail = {\n                assignmentID: item.id,\n                subjectName: item.subject_name,\n                classroomName: item.class_name,\n                title: item.title,\n                description: item.written_description,\n                dueDatetime: item.due_datetime,\n              }\n\n              this.assignments.push(assignmentDetail);\n            }\n\n          })\n    },\n    getHumanDate(datetime) {\n      return moment(datetime, \"YYYY-MM-DD hh:mm:ss\").format(\"DD MMMM YYYY\")\n    }\n  },\n\n  mounted() {\n    this.getAssignments()\n  },\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":";AA0BA,OAAOA,MAAK,MAAO,QAAQ;AAC3B,OAAOC,i