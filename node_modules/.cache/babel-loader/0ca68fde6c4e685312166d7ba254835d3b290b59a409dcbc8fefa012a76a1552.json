{"ast":null,"code":"import IconBase from \"@/components/IconBase\";\nimport ProfilePhoto from \"@/components/icons/ProfilePhoto\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\nimport IconBaseTwo from \"@/components/IconBaseTwo\";\nimport GoldMedalIcon from \"@/components/icons/GoldMedalIcon\";\nimport LayoutOne from \"@/views/students/StudentClass/Components/LayoutOne\";\nexport default {\n  name: \"ClassRanking\",\n  data() {\n    return {\n      rankings: [],\n      totalNumOfStudents: 0\n    };\n  },\n  methods: {\n    getRankings() {\n      StudentRepository.getClassRanking().then(response => {\n        if (response.data.success) {\n          let data = response.data.data;\n          this.totalNumOfStudents = data.total_students;\n          this.rankings = data.rankings;\n        }\n      });\n    }\n  },\n  mounted() {\n    this.getRankings();\n  },\n  components: {\n    LayoutOne,\n    GoldMedalIcon,\n    IconBaseTwo,\n    ProfilePhoto,\n    IconBase\n  }\n};","map":{"version":3,"names":["IconBase","ProfilePhoto","StudentRepository","IconBaseTwo","GoldMedalIcon","LayoutOne","name","data","rankings","totalNumOfStudents","methods","getRankings","getClassRanking","then","response","success","total_students","mounted","components"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/students/StudentClass/Components/ClassRanking.vue"],"sourcesContent":["<template>\n\n  <layout-one>\n    <!-- Subject Title + Period -->\n    <div class=\"text-purple-primary font-bold text-left text-sm bg-gray-secondary py-2 px-5 mb-5\">\n      Class Ranking (Jan - Dec {{ $store.getters.currentYear }})\n    </div>\n\n    <div class=\"px-5\">\n\n      <router-link v-for=\"(student, index) in rankings\"\n                   :key=\"student.student.student_id\"\n                   :to=\"{name : 'student.profile.show', params: {studentID: student.student.student_id} }\"\n                   class=\"mb-3 w-full rounded rounded-xl overflow-hidden bg-gray-secondary flex flex-row pl-1\"\n      >\n\n        <div class=\"flex flex-row w-4/12 items-center\">\n          <!-- Ranking -->\n          <div :class=\"student.ranking === 1 ? 'items-start' : 'justify-center px-2'\"\n               class=\" flex flex-col items-center w-3/7 h-full  text-center mr-1 \">\n\n            <icon-base-two v-if=\"student.ranking === 1\" class=\"w-full md:w-6/7\">\n              <gold-medal-icon/>\n            </icon-base-two>\n\n            <div v-else class=\"font-bold text-lg md:text-3xl\">\n              {{ student.ranking }}\n            </div>\n          </div>\n\n          <!-- Student photo -->\n          <icon-base-two class=\"w-3/7\">\n            <profile-photo :gender=\"student.student.gender\"/>\n          </icon-base-two>\n        </div>\n\n        <div class=\"flex w-6/12 py-3 md:py-6\">\n\n          <!-- Student Details -->\n          <div class=\"flex flex-col w-full justify-between\">\n            <div>\n              <div class=\"text-left text-purple-primary text-xs-plus font-bold  truncate  pr-5\">\n                {{ student.student.student_name || '' }}\n              </div>\n\n              <div class=\"text-left text-purple-secondary text-px-10  truncate mt-2 pr-5\">\n                {{ student.student.class_name || '' }}\n              </div>\n            </div>\n          </div>\n        </div>\n\n        <div class=\"w-3/12 flex flex-col justify-center font-bold text-sm text-right pr-5 md:text-lg \">\n          {{ `${student.student.marks_average}%`}}\n        </div>\n      </router-link>\n    </div>\n  </layout-one>\n</template>\n\n<script>\nimport IconBase from \"@/components/IconBase\";\nimport ProfilePhoto from \"@/components/icons/ProfilePhoto\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\nimport IconBaseTwo from \"@/components/IconBaseTwo\";\nimport GoldMedalIcon from \"@/components/icons/GoldMedalIcon\";\nimport LayoutOne from \"@/views/students/StudentClass/Components/LayoutOne\";\n\nexport default {\n  name: \"ClassRanking\",\n  data() {\n    return {\n      rankings: [],\n      totalNumOfStudents: 0,\n    }\n  },\n  methods: {\n    getRankings() {\n      StudentRepository.getClassRanking()\n          .then(response => {\n            if (response.data.success) {\n              let data = response.data.data;\n\n              this.totalNumOfStudents = data.total_students;\n              this.rankings = data.rankings\n            }\n          })\n    }\n  },\n  mounted() {\n    this.getRankings()\n  },\n  components: {LayoutOne, GoldMedalIcon, IconBaseTwo, ProfilePhoto, IconBase}\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":"AA6DA,OAAOA,QAAO,MAAO,uBAAuB;AAC5C,OAAOC,YAAW,MAAO,iCAAiC;AAC1D,OAAOC,iBAAgB,MAAO,kCAAkC;AAChE,OAAOC,WAAU,MAAO,0BAA0B;AAClD,OAAOC,aAAY,MAAO,kCAAkC;AAC5D,OAAOC,SAAQ,MAAO,oDAAoD;AAE1E,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,kBAAkB,EAAE;IACtB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZT,iBAAiB,CAACU,eAAe,EAAC,CAC7BC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACP,IAAI,CAACQ,OAAO,EAAE;UACzB,IAAIR,IAAG,GAAIO,QAAQ,CAACP,IAAI,CAACA,IAAI;UAE7B,IAAI,CAACE,kBAAiB,GAAIF,IAAI,CAACS,cAAc;UAC7C,IAAI,CAACR,QAAO,GAAID,IAAI,CAACC,QAAO;QAC9B;MACF,CAAC;IACP;EACF,CAAC;EACDS,OAAOA,CAAA,EAAG;IACR,IAAI,CAACN,WAAW,EAAC;EACnB,CAAC;EACDO,UAAU,EAAE;IAACb,SAAS;IAAED,aAAa;IAAED,WAAW;IAAEF,YAAY;IAAED;EAAQ;AAC5E"},"metadata":{},"sourceType":"module","externalDependencies":[]}