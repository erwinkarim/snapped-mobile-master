 response.data.meta.total;\n        }\n      });\n      TeacherRepository.getSubmissions().then(response => {\n        if (response.data.success) {\n          this.numOfSubmissions = response.data.meta.total;\n        }\n      });\n      TeacherRepository.getMarkings().then(response => {\n        this.numOfUnmarkedSubmissions = response.data.num_of_unmarked_submissions;\n      });\n    },\n    fetchSubmissions() {\n      TeacherRepository.getSubmissions(1).then(response => {\n        let data = response.data.data;\n        for (let i = 0; i < data.length; i++) {\n          let submission = data[i];\n          let details = {\n            id: submission.submission_id,\n            assignmentID: submission.assignment_id,\n            studentID: submission.student_id,\n            studentName: submission.student_name,\n            studentGender: submission.gender,\n            submittedAt: submission.submission_created_at,\n            classroomName: submission.classroom_name,\n            subjectName: submission.subject_name\n          };\n          this.submissions.push(details);\n        }\n      });\n    }\n  },\n  mounted() {\n    this.fetchSubmissions();\n    this.fetchSummaryData();\n  },\n  components: {\n    IconBaseTwo,\n    AssignmentSubmissionCard,\n    UserProfile,\n    SubmissionCard,\n    DashboardLayout,\n    AssignmentSwiper,\n    ProfilePhoto,\n    ClassesSwiper,\n    SectionTitle,\n    AppLogo,\n    IconBase,\n    PlusIcon,\n    EdCoinSummaryPill\n  }\n};","map":{"version":3,"names":["IconBase","AppLogo","PlusIcon","SectionTitle","ClassesSwiper","ProfilePhoto","AssignmentSwiper","DashboardLayout","SubmissionCard","UserProfile","AssignmentSubmissionCard","SubmissionRepository","TeacherRepository","IconBaseTwo","AssignmentRepository","EdCoinSummaryPill","name","data","numOfPublishedAssignments","numOfAssignments","numOfSubmissions","numOfUnmarkedSubmissions","submissions","methods","getNumOfAssignments","value","fetchSummaryData","getPublishedAssignmen