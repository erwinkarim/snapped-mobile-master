{"ast":null,"code":"import IconBaseTwo from \"@/components/IconBaseTwo\";\nimport GoldMedalIcon from \"@/components/icons/GoldMedalIcon\";\nimport ProfilePhoto from \"@/components/icons/ProfilePhoto\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\nimport InfiniteLoading from \"vue-infinite-loading\";\nexport default {\n  name: \"SchoolRanking\",\n  methods: {\n    handleSchoolRankingData($state) {\n      this.$store.dispatch('studentRanking/handleSchoolRankingData').then(status => {\n        if (status === 'loaded') {\n          $state.loaded();\n        } else if (status === 'completed') {\n          $state.complete();\n        }\n      });\n    }\n  },\n  components: {\n    ProfilePhoto,\n    GoldMedalIcon,\n    IconBaseTwo,\n    InfiniteLoading\n  }\n};","map":{"version":3,"names":["IconBaseTwo","GoldMedalIcon","ProfilePhoto","StudentRepository","InfiniteLoading","name","methods","handleSchoolRankingData","$state","$store","dispatch","then","status","loaded","complete","components"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/students/StudentRanking/SchoolRanking.vue"],"sourcesContent":["<template>\n  <div class=\"px-5\">\n    <div class=\"text-purple-primary font-bold min-w-full text-left text-sm bg-gray-secondary py-2 px-5 mb-5\">\n      School Ranking (Jan - Dec {{ $store.getters.currentYear }})\n    </div>\n    <router-link v-for=\"(student, index) in $store.state.studentRanking.schoolRankings.data\"\n                 :key=\"student.student_id\"\n                 :to=\"{name : 'student.profile.show', params: {studentID: student.student_id} }\"\n                 class=\"mb-3 w-full rounded rounded-xl overflow-hidden bg-gray-secondary flex flex-row pl-1\">\n\n      <div class=\"flex flex-row w-3/12 items-center\">\n        <!-- Ranking -->\n        <div :class=\"student.ranking === 1 ? 'items-start' : 'justify-center px-2'\"\n             class=\" flex flex-col w-3/7 h-full text-center mr-1 \">\n\n          <icon-base-two v-if=\"student.ranking === 1\" class=\"w-full\">\n            <gold-medal-icon/>\n          </icon-base-two>\n\n          <div v-else class=\"font-bold text-lg md:text-3xl\">\n            {{ student.ranking }}\n          </div>\n        </div>\n\n        <!-- Student photo -->\n        <icon-base-two class=\"w-3/7\">\n          <profile-photo :gender=\"student.student_gender\"/>\n        </icon-base-two>\n      </div>\n\n      <div class=\"flex w-6/12 py-3\">\n\n        <!-- Student Details -->\n        <div class=\"flex flex-col w-full justify-between\">\n          <div>\n            <div class=\"text-left text-purple-primary text-xs-plus font-bold  truncate  pr-5\">\n              {{ student.student_name }}\n            </div>\n\n            <div class=\"text-left text-purple-secondary text-px-10 truncate mt-2 pr-5\">\n              {{ student.class_name }}\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"w-3/12 flex flex-col justify-center font-semibold text-sm text-right pr-5 md:text-lg \">\n        {{ `${student.marks_average}%` }}\n      </div>\n    </router-link>\n\n    <infinite-loading :identifier=\"$store.state.studentRanking.schoolRankings.infiniteScrollIdentifier\"\n                      @infinite=\"handleSchoolRankingData\"\n                      spinner=\"bubbles\"\n                      force-use-infinite-wrapper\n    >\n      <div slot=\"spinner\" class=\"mt-10\">Loading...</div>\n      <div slot=\"no-more\"></div>\n      <div slot=\"no-results\">\n        <div v-if=\"!$store.state.studentRanking.schoolRankings.data.length\">\n          No data available\n        </div>\n      </div>\n    </infinite-loading>\n  </div>\n</template>\n\n<script>\nimport IconBaseTwo from \"@/components/IconBaseTwo\";\nimport GoldMedalIcon from \"@/components/icons/GoldMedalIcon\";\nimport ProfilePhoto from \"@/components/icons/ProfilePhoto\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\nimport InfiniteLoading from \"vue-infinite-loading\";\n\nexport default {\n  name: \"SchoolRanking\",\n  methods: {\n    handleSchoolRankingData($state) {\n      this.$store.dispatch('studentRanking/handleSchoolRankingData')\n          .then(status => {\n            if (status === 'loaded') {\n              $state.loaded();\n            } else if(status === 'completed') {\n              $state.complete();\n            }\n          })\n    }\n  },\n  components: {ProfilePhoto, GoldMedalIcon, IconBaseTwo, InfiniteLoading}\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":"AAoEA,OAAOA,WAAU,MAAO,0BAA0B;AAClD,OAAOC,aAAY,MAAO,kCAAkC;AAC5D,OAAOC,YAAW,MAAO,iCAAiC;AAC1D,OAAOC,iBAAgB,MAAO,kCAAkC;AAChE,OAAOC,eAAc,MAAO,sBAAsB;AAElD,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,OAAO,EAAE;IACPC,uBAAuBA,CAACC,MAAM,EAAE;MAC9B,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,wCAAwC,EACxDC,IAAI,CAACC,MAAK,IAAK;QACd,IAAIA,MAAK,KAAM,QAAQ,EAAE;UACvBJ,MAAM,CAACK,MAAM,EAAE;QACjB,OAAO,IAAGD,MAAK,KAAM,WAAW,EAAE;UAChCJ,MAAM,CAACM,QAAQ,EAAE;QACnB;MACF,CAAC;IACP;EACF,CAAC;EACDC,UAAU,EAAE;IAACb,YAAY;IAAED,aAAa;IAAED,WAAW;IAAEI;EAAe;AACxE"},"metadata":{},"sourceType":"module","externalDependencies":[]}