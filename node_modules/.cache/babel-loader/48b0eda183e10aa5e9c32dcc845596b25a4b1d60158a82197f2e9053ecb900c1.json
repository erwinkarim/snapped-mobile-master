{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SectionTitle from \"@/components/SectionTitle\";\nimport DashboardLayout from \"@/views/layout/DashboardLayout\";\nimport CoinsBottomNavBarVue from \"@/components/CoinsBottomNavBar.vue\";\nimport PageHeaderThree from \"@/components/PageHeaderThree\";\nimport NavBack from \"@/components/NavBack\";\nimport StoresRepository from \"@/repositories/StoresRepository\";\nexport default {\n  name: \"StoresOrdersProcess\",\n  data() {\n    return {\n      msg: ['Processing order', 'Order failed. Check your balance or try again', 'Order successful, attempt to redirect to order page'],\n      msg_status: 0\n    };\n  },\n  mounted() {\n    // process order than push to new page with the result\n\n    StoresRepository.storeNewOrder(this.$route.query).then(res => {\n      console.log('store order new', res);\n      if (res.data !== 0) {\n        // show orders\n        this.msg_status = 2;\n        this.$router.push({\n          name: 'stores.orders.show',\n          params: {\n            orderID: res.data\n          },\n          query: {\n            orderConfirmed: true\n          }\n        });\n      }\n      ;\n    }).catch(res => {\n      // order failed\n      console.log('order failed');\n      this.msg_status = 1;\n    });\n  },\n  components: {\n    SectionTitle,\n    DashboardLayout,\n    CoinsBottomNavBarVue,\n    PageHeaderThree,\n    NavBack\n  }\n};","map":{"version":3,"names":["SectionTitle","DashboardLayout","CoinsBottomNavBarVue","PageHeaderThree","NavBack","StoresRepository","name","data","msg","msg_status","mounted","storeNewOrder","$route","query","then","res","console","log","$router","push","params","orderID","orderConfirmed","catch","components"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/stores/StoresOrdersProcess.vue"],"sourcesContent":["\n<template>\n  <dashboard-layout :has-custom-bottom-bar=\"true\" >\n    <template v-slot:pageHeader>\n      <page-header-three :bottom-padding=\"5\">\n        <template v-slot:leftAction>\n        </template>\n        <template v-slot:mini-title>\n          Processing Order\n        </template>\n      </page-header-three>\n    </template>\n\n    <template v-slot:content>\n      <div class=\"h-30\">Empty Space</div>\n      <h2 class=\"text-2xl\">Processing Your Order ...</h2>\n      <div>Do not refresh or reload this page.</div>\n      <div>{{  msg[msg_status] }}</div>\n    </template>\n\n    <template v-slot:bottomBar>\n      <CoinsBottomNavBarVue />\n    </template>\n  </dashboard-layout>\n\n</template>\n\n<script>\nimport SectionTitle from \"@/components/SectionTitle\";\nimport DashboardLayout from \"@/views/layout/DashboardLayout\";\nimport CoinsBottomNavBarVue from \"@/components/CoinsBottomNavBar.vue\";\nimport PageHeaderThree from \"@/components/PageHeaderThree\";\nimport NavBack from \"@/components/NavBack\";\nimport StoresRepository from \"@/repositories/StoresRepository\";\n\nexport default {\n  name: \"StoresOrdersProcess\",\n  data(){\n\n    return {\n      msg: [\n        'Processing order',\n        'Order failed. Check your balance or try again',\n        'Order successful, attempt to redirect to order page',\n      ],\n      msg_status: 0,\n    };\n  },\n  mounted(){\n    // process order than push to new page with the result\n\n    StoresRepository.storeNewOrder(this.$route.query).then((res) => {\n      console.log('store order new', res);\n    \n      if(res.data !== 0){\n        // show orders\n        this.msg_status = 2;\n        this.$router.push({ name: 'stores.orders.show', params: {orderID: res.data }, query: { orderConfirmed: true }});\n      };\n    }).catch((res) => {\n      // order failed\n      console.log('order failed')\n      this.msg_status = 1;\n    });\n  },\n  components: {\n    SectionTitle, DashboardLayout, CoinsBottomNavBarVue, PageHeaderThree, \n    NavBack,\n  },\n}\n</script>"],"mappings":";AA4BA,OAAOA,YAAW,MAAO,2BAA2B;AACpD,OAAO