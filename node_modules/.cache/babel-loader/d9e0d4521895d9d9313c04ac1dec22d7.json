ject;\n    },\n\n    showResetButton() {\n      return !this.$store.state.teacherMarking.states.isSelectingObject && !this.$store.state.teacherMarking.states.isMovingObject && !this.$store.state.teacherMarking.states.isScalingObject;\n    },\n\n    isViewingMarkingMainPage() {\n      return this.$store.state.teacherMarking.states.isMarking && !this.$store.state.teacherMarking.states.isDrawing\n    },\n\n    headerBackgroundColor: function () {\n\n      let value = 'bg-black-primary';\n\n      if (this.$store.state.teacherMarking.states.isPreviewing || this.$store.state.teacherMarking.states.isWritingFeedback) {\n        value = 'bg-white'\n      }\n      if (this.$store.state.teacherMarking.states.isMarking) {\n        value = 'bg-transparent'\n      }\n\n      return value;\n    },\n\n    drawingModeState() {\n\n      if (this.$store.state.teacherMarking.nowDrawing.drawing && !this.$store.state.teacherMarking.nowDrawing.erasing) {\n        return 'drawing';\n      }\n\n      if (this.$store.state.teacherMarking.nowDrawing.erasing && !this.$store.state.teacherMarking.nowDrawing.drawing) {\n        return 'erasing';\n      }\n    },\n\n\n    navBackColor: function () {\n      if (this.$store.state.teacherMarking.states.isPreviewing) {\n        return 'purple-primary'\n      } else {\n        return 'white'\n      }\n    },\n  },\n  methods: {\n\n    // MODE: PREVIEW\n    togglePreviewMode() {\n      this.$store.commit('teacherMarking/togglePreviewMode')\n    },\n\n\n    exitMarkingMode() {\n      this.$store.commit('teacherMarking/setPreviewingMode')\n      router.push({name: 'teacher.assignments.marking.details'})\n    },\n\n\n    toggleDrawingModeState(action) {\n      if (action === 'drawing' && !this.$store.state.teacherMarking.nowDrawing.drawing) {\n        this.$store.commit('teacherMarking/toggleDrawingModeStates')\n      }\n\n      if (action === 'erasing' && !this.$store.state.teacherMarking.nowDrawing.erasing) {\n\n      }\n    },\n\n    removeSelectedObject(){\n      this.$store.dispatch('teacherMarking/removeSelectedObject');\n    },\n\n    undoEdits() {\n      this.$store.dispatch('teacherMarking/undoEditedSnappedAnswer')\n    }\n  },\n  components: {UndoIcon, ArrowBackIcon, IconBaseTwo, NavBack, PageHeaderThree, FontAwesomeIcon, FontAwesomeLayers}\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":";AAkIA,OAAAA,eAAA;AACA,OAAAC,OAAA;AACA,OAAAC,WAAA;AACA,OAAAC,aAAA;AACA,OAAAC,QAAA;AACA,OAAAC,MAAA;AAEA,SAAAC,QAAA,EAAAC,QAAA,EAAAC,QAAA,EAAAC,OAAA;AACA,SAAAC,eAAA,EAAAC,iBAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;QACAC,MAAA,EAAAT,QAAA;QACAU,MAAA,EAAAT,QAAA;QACAU,MAAA,EAAAT,QAAA;QACAU,KAAA,EAAAT;MACA;IACA;EACA;EACAU,QAAA;IAEAC,cAAA;MACA,YAAAC,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAC,iBAAA;IACA;IAEAC,gBAAA;MACA,aAAAL,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAC,iBAAA,UAAAJ,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAG,cAAA,UAAAN,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAI,eAAA;IACA;IAEAC,yBAAA;MACA,YAAAR,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAM,SAAA,UAAAT,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAO,SAAA;IACA;IAEAC,qBAAA,WAAAA,CAAA;MAEA,IAAAC,KAAA;MAEA,SAAAZ,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAU,YAAA,SAAAb,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAW,iBAAA;QACAF,KAAA;MACA;MACA,SAAAZ,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAM,SAAA;QACAG,KAAA;MACA;MAEA,OAAAA,KAAA;IACA;IAEAG,iBAAA;MAEA,SAAAf,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAc,UAAA,CAAAC,OAAA,UAAAjB,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAc,UAAA,CAAAE,OAAA;QACA;MACA;MAEA,SAAAlB,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAc,UAAA,CAAAE,OAAA,UAAAlB,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAc,UAAA,CAAAC,OAAA;QACA;MACA;IACA;IAGAE,YAAA,WAAAA,CAAA;MACA,SAAAnB,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAC,MAAA,CAAAU,YAAA;QACA;MACA;QACA;MACA;IACA;EACA;EACAO,OAAA;IAEA;IACAC,kBAAA;MACA,KAAArB,MAAA,CAAAsB,MAAA;IACA;IAGAC,gBAAA;MACA,KAAAvB,MAAA,CAAAsB,MAAA;MACAtC,MAAA,CAAAwC,IAAA;QAAAjC,IAAA;MAAA;IACA;IAGAkC,uBAAAC,MAAA;MACA,IAAAA,MAAA,wBAAA1B,MAAA,CAAAC,KAAA,CAAAC,cAAA,CAAAc,UAAA,CAAAC,OAAA;QACA,KAAAjB,MAAA,CAAAsB