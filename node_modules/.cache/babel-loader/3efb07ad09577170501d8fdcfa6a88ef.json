     console.log('store products', res);\n      this.products = res.data;\n    });\n  },\n  components: {\n    SectionTitle, DashboardLayout, CoinsBottomNavBarVue, PageHeaderThree, \n    NavBack, ProductCard,\n  },\n}\n</script>"],"mappings":"AAkCA,OAAAA,YAAA;AACA,OAAAC,eAAA;AACA,OAAAC,oBAAA;AACA,OAAAC,eAAA;AACA,OAAAC,OAAA;AACA,OAAAC,gBAAA;AACA,OAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,KAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACAC,QAAA;MACA,OAAAC,OAAA,CAAAC,GAAA,CAAAC,UAAA,QAAAN,KAAA,CAAAG,OAAA;IACA;EACA;EACAI,QAAA;IACAX,gBAAA,CAAAY,WAAA,MAAAC,MAAA,CAAAC,MAAA,CAAAC,OAAA,EAAAC,IAAA,CAAAC,GAAA;MACA,KAAAb,KAAA,GAAAa,GAAA,CAAAd,IAAA;IACA;;IAEA;IACAH,gBAAA,CAAAkB,aAAA,MAAAL,MAAA,CAAAC,MAAA,CAAAC,OAAA,EAAAC,IAAA,CAAAC,GAAA;MACAE,OAAA,CAAAC,GAAA,mBAAAH,GAAA;MACA,KAAAZ,QAAA,GAAAY,GAAA,CAAAd,IAAA;IACA;EACA;EACAkB,UAAA;IACA1B,YAAA;IAAAC,eAAA;IAAAC,oBAAA;IAAAC,eAAA;IACAC,OAAA;IAAAE;EACA;AACA"}]}