storeRecentProducts().then((res) => {\n      this.products = res.data;\n      console.log('products', res.data);\n    });\n\n    // load stores\n    StoresRepository.storeIndex().then((res) => {\n      this.stores = res.data;\n      console.log('stores', res.data);\n    });\n\n    // load recent orders\n    StoresRepository.storeOrders().then((res) => {\n      this.orders = res.data.orders;\n      this.orderStatusCodes = res.data.status_codes;\n      console.log('orders', res.data);\n    }) ;\n  },\n  components: {\n    SectionTitle, DashboardLayout, CoinsBottomNavBarVue, PageHeaderThree, \n    NavBack, ProductCard, StoreCard, OrderCard,\n  },\n}\n</script>"],"mappings":"AA8CA,OAAOA,YAAW,MAAO,2BAA2B;AACpD,OAAOC,eAAc,MAAO,gCAAgC;AAC5D,OAAOC,oBAAmB,MAAO,oCAAoC;AACrE,OAAOC,eAAc,MAAO,8BAA8B;AAC1D,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,gBAAe,MAAO,iCAAiC;AAC9D,OAAOC,WAAU,MAAO,oCAAoC;AAC5D,OAAOC,SAAQ,MAAO,kCAAkC;AACxD,OAAOC,SAAQ,MAAO,kCAAkC;AAExD,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,iBAAiB;MAC5BC,gBAAgB,EAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,CAAC,EAAC;MACb;MACAC,OAAO,CAACC,GAAG,CAACF,CAAC,CAAC;;MAEd;MACA,IAAI,CAACG,KAAK,CAAC,iBAAiB,CAAC,CAACC,KAAK,CAACC,OAAO,GAAC,MAAM;MAClD,IAAI,CAACF,KAAK,CAAC,eAAe,CAAC,CAACC,KAAK,CAACC,OAAO,GAAC,MAAM;MAChD,IAAI,CAACF,KAAK,CAAC,gBAAgB,CAAC,CAACC,KAAK,CAACC,OAAO,GAAC,MAAM;MACjD,IAAI,CAACF,KAAK,CAACH,CAAC,CAAC,CAACI,KAAK,CAACC,OAAO,GAAC,MAAM;MAClC,IAAI,CAACT,SAAQ,GAAII,CAAC;IACpB;EACF,CAAC;EACDM,OAAOA,CAAA,EAAE;IACP;IACAnB,gBAAgB,CAACoB,mBAAmB,EAAE,CAACC,IAAI,CAAEC,GAAG,IAAK;MACnD,IAAI,CAACf,QAAO,GAAIe,GAAG,CAACjB,IAAI;MACxBS,OAAO,CAACC,GAAG,CAAC,UAAU,EAAEO,GAAG,CAACjB,IAAI,CAAC;IACnC,CAAC,CAAC;;IAEF;IACAL,gBAAgB,CAACuB,UAAU,EAAE,CAACF,IAAI,CAAEC,GAAG,IAAK;MAC1C,IAAI,CAAChB,MAAK,GAAIgB,GAAG,CAACjB,IAAI;MACtBS,OAAO,CAACC,GAAG,CAAC,QAAQ,EAAEO,GAAG,CAACjB,IAAI,CAAC;IACjC,CAAC,CAAC;;IAEF;IACAL,gBAAgB,CAACwB,WAAW,EAAE,CAACH,IAAI,