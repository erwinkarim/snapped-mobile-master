{"ast":null,"code":"import NavBack from \"@/components/NavBack\";\nimport PageTitleTwo from \"@/components/PageTitleTwo\";\nimport Modal from \"@/components/Modal\";\nimport PageHeaderThree from \"@/components/PageHeaderThree\";\nimport DashboardLayout from \"@/views/layout/DashboardLayout\";\nexport default {\n  name: \"AnswerWrite\",\n  props: {\n    answer: Object,\n    assignmentDetails: Object,\n    isEditingAnswer: {\n      type: Boolean,\n      default: false\n    }\n  },\n  computed: {\n    writtenAnswer() {\n      return this.answer.content;\n    },\n    navBackRoute() {\n      if (this.isEditingAnswer) {\n        return {\n          name: 'student.assignments.answer.store',\n          params: {\n            assignmentDetails: this.assignmentDetails\n          }\n        };\n      } else {\n        return {\n          name: 'student.assignments.show'\n        };\n      }\n    }\n  },\n  data() {\n    return {\n      latestAnswer: ''\n    };\n  },\n  methods: {\n    compileAnswer() {\n      this.$emit('writtenAnswer', this.latestAnswer);\n    }\n  },\n  mounted() {\n    this.latestAnswer = this.writtenAnswer;\n  },\n  components: {\n    DashboardLayout,\n    PageHeaderThree,\n    Modal,\n    PageTitleTwo,\n    NavBack\n  }\n};","map":{"version":3,"names":["NavBack","PageTitleTwo","Modal","PageHeaderThree","DashboardLayout","name","props","answer","Object","assignmentDetails","isEditingAnswer","type","Boolean","default","computed","writtenAnswer","content","navBackRoute","params","data","latestAnswer","methods","compileAnswer","$emit","mounted","components"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/students/StudentAssignments/Answer/AnswerWrite.vue"],"sourcesContent":["<template>\n  <dashboard-layout :content-fills-screen=\"true\" :no-bottom-bar=\"true\">\n\n    <template v-slot:pageHeader>\n      <page-header-three>\n\n        <template v-slot:leftAction>\n          <nav-back :to=\"navBackRoute\" class=\"w-2/7\" stroke-c