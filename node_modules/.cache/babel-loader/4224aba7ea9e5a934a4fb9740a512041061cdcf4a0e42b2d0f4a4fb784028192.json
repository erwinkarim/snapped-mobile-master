{"ast":null,"code":"export default {\n  props: {\n    product: {},\n    img: \"https://via.placeholder.com/150\"\n  },\n  computed: {\n    picture() {\n      return process.env.VUE_APP_S3 + this.product.picture;\n    }\n  },\n  name: \"ProductCard\"\n};","map":{"version":3,"names":["props","product","img","computed","picture","process","env","VUE_APP_S3","name"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/components/Store/ProductCard.vue"],"sourcesContent":["<template>\n  <div class=\"bg-gray-secondary my-2\">\n    <router-link :to=\"{ name:'stores.product.show', params: { storeID: product.store_id, productID: product.id }}\">\n      <img v-if=\" product.picture != '' \" :src=\"picture\" class=\"aspect-square w-full\" crossorigin=\"anonymous\" />\n      <img v-else src=\"@/assets/img/150.png\" class=\"aspect-square w-full\" />\n      <div class=\"p-2\">\n        <h2 class=\"text-lg font-bold\">{{  product.name }}</h2>\n        <p>{{ product.price }} EdCoins</p>\n        <small v-if=\"product.store != null\">By {{ product.store.name }}</small>\n        <small v-else>By {{ product.store_id }}</small>\n      </div>\n    </router-link>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  props: {\n    product: {},\n    img: \"https://via.placeholder.com/150\",\n  },\n  computed: {\n    picture(){\n      return process.env.VUE_APP_S3 + this.product.picture;\n    }\n  },\n  name: \"ProductCard\",\n}\n</script>"],"mappings":"AAiBA,eAAe;EACbA,KAAK,EAAE;IACLC,OAAO,EAAE,CAAC,CAAC;IACXC,GAAG,EAAE;EACP,CAAC;EACDC,QAAQ,EAAE;IACRC,OAAOA,CAAA,EAAE;MACP,OAAOC,OAAO,CAACC,GAAG,CAACC,UAAS,GAAI,IAAI,CAACN,OAAO,CAACG,OAAO;IACtD;EACF,CAAC;EACDI,IAAI,EAAE;AACR"},"metadata":{},"sourceType":"module","externalDependencies":[]}