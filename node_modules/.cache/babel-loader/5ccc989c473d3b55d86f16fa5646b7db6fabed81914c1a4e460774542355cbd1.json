{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport DashboardLayout from \"@/views/layout/DashboardLayout\";\nimport SectionTitle from \"@/components/SectionTitle\";\nimport PageHeaderThree from \"@/components/PageHeaderThree\";\nimport NavBack from \"@/components/NavBack\";\nimport PageTitle from \"@/components/PageTitle\";\nimport CoinsBottomNavBarVue from \"@/components/CoinsBottomNavBar.vue\";\nimport CoinsRepository from \"@/repositories/CoinsRepository\";\nexport default {\n  data() {\n    return {\n      marks: []\n    };\n  },\n  name: \"CoinTopRedeemMarks\",\n  mounted() {\n    // get a list of assignments\n    console.log('coinTopUpBuy mounted');\n    CoinsRepository.marksForCoinsList().then(response => {\n      console.log(response);\n      this.marks = response.data.marks;\n    });\n  },\n  computed: {\n    availableMarks() {\n      if (this.marks.length === 0) {\n        return 0;\n      } else {\n        return this.marks.reduce((a, v) => {\n          if (v.amount_redeemed === null) {\n            return a += v.final_marks;\n          } else {\n            return a;\n          }\n        }, 0);\n      }\n    }\n  },\n  methods: {\n    requestTopUp() {\n      console.log('request top up');\n\n      // plan: send request to server, get ack from server, then move to ack page.\n      // otherwise, give alert that it didn't work.\n      this.$router.push({\n        name: 'coin.top_up_buy_ack'\n      });\n    }\n  },\n  components: {\n    DashboardLayout,\n    SectionTitle,\n    PageHeaderThree,\n    NavBack,\n    PageTitle,\n    CoinsBottomNavBarVue\n  }\n};","map":{"version":3,"names":["DashboardLayout","SectionTitle","PageHeaderThree","NavBack","PageTitle","CoinsBottomNavBarVue","CoinsRepository","data","marks","name","mounted","console","log","marksForCoinsList","then","response","computed","availableMarks","length","reduce","a","v","amount_redeemed","final_marks","methods","requestTopUp","$router","push","components"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/edcoins/CoinTopUpRedeemMarks.vue"],"sourcesContent":["<template>\n  <dashboard-layout :has-fixed-header=\"true\" :has-custom-bottom-bar=\"true\">\n    <template v-slot:pageHeader>\n      <page-header-three :bottom-padding=\"5\">\n        <template v-slot:leftAction>\n          <nav-back :to=\"{name: 'coin.top_up'}\" class=\"w-2/7\" stroke-color=\"red-primary\"/>\n        </template>\n        <template v-slot:mini-title>\n          Redeem Marks for Coins\n        </template>\n      </page-header-three>\n    </template>\n\n    <template v-slot:content>\n      <div class=\"h-30\">Empty Space</div>\n      <div class=\"px-5 w-full md:px-10\">\n        <div class=\"mt-2 mb-3\">\n          <div class=\"overflow-hidden justify-between py-3 px-3 max-w-sm md:max-w-xl h-full rounded rounded-xl bg-gray-secondary\">\n            <div class=\"flex flex-row items-center h-full text-left text-purple-primary\">\n              You can redeem up to {{ availableMarks }} coins\n            </div>\n          </div>\n        </div>\n      </div>\n      <div class=\"px-5 w-full md:px-10\">\n        <div v-for=\"mark in marks\" class=\"mb-3 overflow-hidden justify-between max-w-sm md:max-w-xl w-full h-full rounded rounded-xl bg-gray-secondary\">\n          <div class=\"flex flex-row items-center h-full text-left text-purple-primary p-3\">\n            <div class=\"flex w-full text-lg text-center\">Assignment: {{ mark.title }} ({{ mark.assignment_id }}) </div>\n          </div>\n          <div class=\"flex flex-row items-center h-full text-left text-purple-primary\">\n            <div class=\"w-1/2 text-center\">Marks<br /> <span class=\"text-4xl\">{{ mark.final_marks }}</span></div>\n            <div class=\"w-1/2 text-center\">Amount Redeemed<br /> <span class=\"text-4xl\">{{ mark.amount_redeemed || 0 }}</span> </div>\n          </div>\n          <div class=\"flex flex-row items-center h-full text-left text-purple-primary p-3 border-t-1 border-gray-500\">\n            <router-link class=\"w-1/2 text-center border-r-1 border-gray-500\" :to=\"{ name: 'coin.top_up_redeem_marks_detail', params: { mark_id: mark.id } }\">Detail</router-link>\n            <router-link class=\"w-1/2 text-center\" :to=\"{ name: 'coin.top_up_redeem_marks_attempt', params: { mark_id: mark.id } }\">Redeem</router-link>\n          </div>\n        </div>\n      </div>\n    </template>\n\n    <template v-slot:bottomBar>\n      <CoinsBottomNavBarVue />\n    </template>\n\n  </dashboard-layout>\n</template>\n\n<script>\nimport DashboardLayout from \"@/views/layout/DashboardLayout\";\nimport SectionTitle from \"@/components/SectionTitle\";\nimport PageHeaderThree from \"@/components/PageHeaderThree\";\nimport NavBack from \"@/components/NavBack\";\nimport PageTitle from \"@/components/PageTitle\";\nimport CoinsBottomNavBarVue from \"@/components/CoinsBottomNavBar.vue\";\nimport CoinsRepository from \"@/repositories/CoinsRepository\";\n\nexport default {\n  data() {\n    return {\n      marks: [],\n    };\n  },\n  name: \"CoinTopRedeemMarks\",\n  mounted() {\n    // get a list of assignments\n    console.log('coinTopUpBuy mounted');\n    CoinsRepository.marksForCoinsList().then((response) => {\n      console.log(response);\n      this.marks = response.data.marks;\n    });\n  },\n  computed: {\n    availableMarks(){\n      if(this.marks.length === 0){\n        return 0;\n      } else {\n        return this.marks.reduce((a, v) => {\n          if(v.amount_redeemed === null){\n            return a += v.final_marks;\n          } else {\n            return a;\n          }\n        }, 0);\n      }\n    },\n  },\n  methods: {\n    requestTopUp() {\n      console.log('request top up');\n      \n      // plan: send request to server, get ack from server, then move to ack page.\n      // otherwise, give alert that it didn't work.\n      this.$router.push({ name: 'coin.top_up_buy_ack'});\n    }\n  },\n  components: {\n    DashboardLayout, SectionTitle, PageHeaderThree, NavBack, PageTitle,\n    CoinsBottomNavBarVue,\n  }\n}\n</script>\n"],"mappings":";AAiDA,OAAOA,eAAc,MAAO,gCAAgC;AAC5D,OAAOC,YAAW,MAAO,2BAA2B;AACpD,OAAOC,eAAc,MAAO,8BAA8B;AAC1D,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,SAAQ,MAAO,wBAAwB;AAC9C,OAAOC,oBAAmB,MAAO,oCAAoC;AACrE,OAAOC,eAAc,MAAO,gCAAgC;AAE5D,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,IAAI,EAAE,oBAAoB;EAC1BC,OAAOA,CAAA,EAAG;IACR;IACAC,OAAO,CAACC,GAAG,CAAC,sBAAsB,CAAC;IACnCN,eAAe,CAACO,iBAAiB,EAAE,CAACC,IAAI,CAAEC,QAAQ,IAAK;MACrDJ,OAAO,CAACC,GAAG,CAACG,QAAQ,CAAC;MACrB,IAAI,CAACP,KAAI,GAAIO,QAAQ,CAACR,IAAI,CAACC,KAAK;IAClC,CAAC,CAAC;EACJ,CAAC;EACDQ,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAE;MACd,IAAG,IAAI,CAACT,KAAK,CAACU,MAAK,KAAM,CAAC,EAAC;QACzB,OAAO,CAAC;MACV,OAAO;QACL,OAAO,IAAI,CAACV,KAAK,CAACW,MAAM,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;UACjC,IAAGA,CAAC,CAACC,eAAc,KAAM,IAAI,EAAC;YAC5B,OAAOF,CAAA,IAAKC,CAAC,CAACE,WAAW;UAC3B,OAAO;YACL,OAAOH,CAAC;UACV;QACF,CAAC,EAAE,CAAC,CAAC;MACP;IACF;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACbd,OAAO,CAACC,GAAG,CAAC,gBAAgB,CAAC;;MAE7B;MACA;MACA,IAAI,CAACc,OAAO,CAACC,IAAI,CAAC;QAAElB,IAAI,EAAE;MAAqB,CAAC,CAAC;IACnD;EACF,CAAC;EACDmB,UAAU,EAAE;IACV5B,eAAe;IAAEC,YAAY;IAAEC,eAAe;IAAEC,OAAO;IAAEC,SAAS;IAClEC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}