{"ast":null,"code":"import SectionTitle from \"@/components/SectionTitle\";\nimport DashboardLayout from \"@/views/layout/DashboardLayout\";\nimport CoinsBottomNavBarVue from \"@/components/CoinsBottomNavBar.vue\";\nimport PageHeaderThree from \"@/components/PageHeaderThree\";\nimport NavBack from \"@/components/NavBack\";\nimport StoresRepository from \"@/repositories/StoresRepository\";\nexport default {\n  name: 'StoresOrdersNew',\n  data() {\n    return {\n      product: {}\n    };\n  },\n  mounted() {\n    StoresRepository.storeProduct(this.$route.query.storeID, this.$route.query.productID).then(res => {\n      this.product = res.data;\n    });\n  },\n  components: {\n    SectionTitle,\n    DashboardLayout,\n    CoinsBottomNavBarVue,\n    PageHeaderThree,\n    NavBack\n  }\n};","map":{"version":3,"names":["SectionTitle","DashboardLayout","CoinsBottomNavBarVue","PageHeaderThree","NavBack","StoresRepository","name","data","product","mounted","storeProduct","$route","query","storeID","productID","then","res","components"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/stores/StoresOrdersNew.vue"],"sourcesContent":["\n<template>\n  <dashboard-layout class=\"pt-5\" :has-custom-bottom-bar=\"true\" >\n    <template v-slot:pageHeader>\n      <page-header-three :bottom-padding=\"5\">\n        <template v-slot:leftAction>\n          <nav-back :to=\"{name: 'stores.product.show', params: {storeID: 1, productID: 1}}\" class=\"w-2/7\" stroke-color=\"red-primary\"/>\n        </template>\n        <template v-slot:mini-title>\n          New Order Page\n        </template>\n      </page-header-three>\n    </template>\n\n    <template v-slot:content>\n      <div class=\"h-30\">Empty Space</div>\n      <div v-if=\"Object.keys(product).length === 0\">\n        <p>No product</p>\n      </div>\n      <div v-else class=\"p-5 text-center\">\n        <img class=\"w-full\" src=\"@/assets/img/300.jpeg\" />\n        <p>You are ordering \"<strong>{{ product.name }}</strong>\" for {{ product.price }} Edcoins.</p>\n        <p class=\"p-4\"><router-link class=\"bg-purple-primary text-white opacity-100 hover:opacity-75 text-lg font-bold py-2 px-4 rounded-lg w-full\" :to=\"{ name: 'stores.orders.process', query: $route.query }\">Initiate Order</router-link></p>\n      </div>\n    </template>\n\n    <template v-slot:bottomBar>\n      <CoinsBottomNavBarVue />\n    </template>\n  </dashboard-layout>\n  \n</template>\n\n<script>\nimport SectionTitle from \"@/components/SectionTitle\";\nimport DashboardLayout from \"@/views/layout/DashboardLayout\";\nimport CoinsBottomNavBarVue from \"@/components/CoinsBottomNavBar.vue\";\nimport PageHeaderThree from \"@/components/PageHeaderThree\";\nimport NavBack from \"@/components/NavBack\";\nimport StoresRepository from \"@/repositories/StoresRepository\";\n\nexport default {\n  name: 'StoresOrdersNew',\n  data(){\n    return { product: {}, }\n  },\n  mounted() {\n    StoresRepository.storeProduct(this.$route.query.storeID, this.$route.query.productID).then((res) => {\n      this.product = res.data;\n    })\n\n  },\n  components: {\n    SectionTitle, DashboardLayout, CoinsBottomNavBarVue, PageHeaderThree, \n    NavBack,\n  },\n}\n</script>"],"mappings":"AAkCA,OAAOA,YAAW,MAAO,2BAA2B;AACpD,OAAOC,eAAc,MAAO,gCAAgC;AAC5D,OAAOC,oBAAmB,MAAO,oCAAoC;AACrE,OAAOC,eAAc,MAAO,8BAA8B;AAC1D,OAAOC,OAAM,MAAO,sBAAsB;AAC1C,OAAOC,gBAAe,MAAO,iCAAiC;AAE9D,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAE;IACJ,OAAO;MAAEC,OAAO,EAAE,CAAC;IAAG;EACxB,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRJ,gBAAgB,CAACK,YAAY,CAAC,IAAI,CAACC,MAAM,CAACC,KAAK,CAACC,OAAO,EAAE,IAAI,CAACF,MAAM,CAACC,KAAK,CAACE,SAAS,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;MAClG,IAAI,CAACR,OAAM,GAAIQ,GAAG,CAACT,IAAI;IACzB,CAAC;EAEH,CAAC;EACDU,UAAU,EAAE;IACVjB,YAAY;IAAEC,eAAe;IAAEC,oBAAoB;IAAEC,eAAe;IACpEC;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}