{"ast":null,"code":"import IconBaseTwo from \"@/components/IconBaseTwo\";\nimport GoldMedalIcon from \"@/components/icons/GoldMedalIcon\";\nimport ProfilePhoto from \"@/components/icons/ProfilePhoto\";\nimport InfiniteLoading from \"vue-infinite-loading\";\nexport default {\n  name: \"NationalRanking\",\n  methods: {\n    handleNationalRankingData($state) {\n      this.$store.dispatch('studentRanking/handleNationalRankingData').then(status => {\n        if (status === 'loaded') {\n          $state.loaded();\n        } else if (status === 'completed') {\n          $state.complete();\n        }\n      });\n    }\n  },\n  components: {\n    ProfilePhoto,\n    GoldMedalIcon,\n    IconBaseTwo,\n    InfiniteLoading\n  }\n};","map":{"version":3,"names":["IconBaseTwo","GoldMedalIcon","ProfilePhoto","InfiniteLoading","name","methods","handleNationalRankingData","$state","$store","dispatch","then","status","loaded","complete","components"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/students/StudentRanking/NationalRanking.vue"],"sourcesContent":["<template>\n  <div class=\"px-5\">\n    <div class=\"text-purple-primary font-bold text-left text-sm bg-gray-secondary py-2 px-5 mb-5\">\n      National Ranking (Jan - Dec {{ $store.getters.currentYear }})\n    </div>\n    <router-link v-for=\"(student, index) in $store.state.studentRanking.nationalRankings.data\"\n                 :key=\"student.student_id\"\n                 :to=\"{name : 'student.profile.show', params: {studentID: student.student_id} }\"\n                 class=\"mb-3 w-full rounded rounded-xl overflow-hidden bg-gray-secondary flex flex-row pl-1\">\n\n      <div class=\"flex flex-row w-3/12 items-center\">\n        <!-- Ranking -->\n\n\n        <div v-if=\"student.ranking === 1\"\n             class=\" flex flex-col w-3/7 h-full text-center mr-1 items-start\"\n        >\n          <icon-base-two class=\"w-full\">\n            <gold-medal-icon/>\n          </icon-base-two>\n        </div>\n        <div v-else\n             class=\" flex flex-col w-3/7 h-full text-center mr-1 justify-center px-2\"\n        >\n          <div class=\"font-bold text-lg md:text-3xl\">\n            {{ student.ranking }}\n          </div>\n        </div>\n\n        <!-- Student photo -->\n        <icon-base-two class=\"w-3/7\">\n          <profile-photo :gender=\"student.student_gender\"/>\n        </icon-base-two>\n      </div>\n\n      <div class=\"flex w-6/12 py-3\">\n\n        <!-- Student Details -->\n        <div class=\"flex flex-col w-full justify-between\">\n          <div>\n            <div class=\"text-left text-purple-primary text-xs-plus font-bold  truncate  pr-5\">\n              {{ student.student_name }}\n            </div>\n\n            <div class=\"text-left text-purple-secondary text-px-10 truncate mt-2 pr-5\">\n              {{ student.school_name }}\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"w-3/12 flex flex-col justify-center font-semibold text-sm text-right pr-5 md:text-lg \">\n        {{ `${student.marks_average}%` }}\n      </div>\n    </router-link>\n\n    <infinite-loading :identifier=\"$store.state.studentRanking.nationalRankings.infiniteScrollIdentifier\"\n                      @infinite=\"handleNationalRankingData\"\n                      spinner=\"bubbles\"\n                      force-use-infinite-wrapper\n    >\n      <div slot=\"spinner\" class=\"mt-10\">Loading...</div>\n      <div slot=\"no-more\"></div>\n      <div slot=\"no-results\">\n        <div v-if=\"!$store.state.studentRanking.nationalRankings.data.length\">\n          No data available\n        </div>\n      </div>\n    </infinite-loading>\n  </div>\n</template>\n\n<script>\n\nimport IconBaseTwo from \"@/components/IconBaseTwo\";\nimport GoldMedalIcon from \"@/components/icons/GoldMedalIcon\";\nimport ProfilePhoto from \"@/components/icons/ProfilePhoto\";\nimport InfiniteLoading from \"vue-infinite-loading\";\n\nexport default {\n  name: \"NationalRanking\",\n  methods: {\n    handleNationalRankingData($state) {\n      this.$store.dispatch('studentRanking/handleNationalRankingData')\n          .then(status => {\n            if (status === 'loaded') {\n              $state.loaded();\n            } else if (status === 'completed') {\n              $state.complete();\n            }\n          })\n    }\n  },\n  components: {ProfilePhoto, GoldMedalIcon, IconBaseTwo, InfiniteLoading}\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":"AA0EA,OAAOA,WAAU,MAAO,0BAA0B;AAClD,OAAOC,aAAY,MAAO,kCAAkC;AAC5D,OAAOC,YAAW,MAAO,iCAAiC;AAC1D,OAAOC,eAAc,MAAO,sBAAsB;AAElD,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,OAAO,EAAE;IACPC,yBAAyBA,CAACC,MAAM,EAAE;MAChC,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,0CAA0C,EAC1DC,IAAI,CAACC,MAAK,IAAK;QACd,IAAIA,MAAK,KAAM,QAAQ,EAAE;UACvBJ,MAAM,CAACK,MAAM,EAAE;QACjB,OAAO,IAAID,MAAK,KAAM,WAAW,EAAE;UACjCJ,MAAM,CAACM,QAAQ,EAAE;QACnB;MACF,CAAC;IACP;EACF,CAAC;EACDC,UAAU,EAAE;IAACZ,YAAY;IAAED,aAAa;IAAED,WAAW;IAAEG;EAAe;AACxE"},"metadata":{},"sourceType":"module","externalDependencies":[]}