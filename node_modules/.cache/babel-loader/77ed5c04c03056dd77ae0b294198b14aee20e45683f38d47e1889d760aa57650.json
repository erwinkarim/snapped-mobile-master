{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport TeacherRepository from \"@/repositories/TeacherRepository\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\nimport moment from \"moment\";\nimport CircleProgressBar from \"@/components/CircleProgressBar\";\nimport AssignmentScoreCircle from \"@/views/students/StudentDetails/components/AssignmentScoreCircle\";\nimport router from \"@/router\";\nexport default {\n  name: \"StudentAssignments\",\n  components: {\n    AssignmentScoreCircle,\n    CircleProgressBar\n  },\n  props: {\n    userRole: String,\n    isAuthStudent: Boolean,\n    studentID: [String, Number]\n  },\n  data() {\n    return {\n      // states\n      error: null,\n      loading: false,\n      awaitingSearch: false,\n      // data\n      assignments: []\n    };\n  },\n  watch: {\n    '$route': 'fetchData'\n  },\n  methods: {\n    fetchData() {\n      this.error = this.assignments = null;\n      this.loading = true;\n      StudentRepository.getMarkedAssignments(this.studentID).then(response => {\n        this.assignments = response.data.data;\n        this.loading = false;\n      });\n    },\n    getHumanDate(datetime) {\n      return moment(datetime, \"YYYY-MM-DD hh:mm:ss\").format(\"DD MMMM YYYY\");\n    }\n  },\n  mounted() {\n    this.fetchData();\n  },\n  created() {\n    if (this.userRole !== 'Teacher' && !this.isAuthStudent) {\n      router.push({\n        name: 'student.profile.show'\n      });\n    }\n  }\n};","map":{"version":3,"names":["TeacherRepository","StudentRepository","moment","CircleProgressBar","AssignmentScoreCircle","router","name","components","props","userRole","String","isAuthStudent","Boolean","studentID","Number","data","error","loading","awaitingSearch","assignments","watch","methods","fetchData","getMarkedAssignments","then","response","getHumanDate","datetime","format","mounted","created","push"],"sources":["/Users/erwinkarim/Websites/Snapped/snapped-mobile-master/src/views/students/StudentDetails/components/StudentAssignments.vue"],"sourcesContent":["<template>\n  <div class=\"px-5 md:mt-4\">\n    <router-link v-for=\"assignment in assignments\"\n                 :key=\"assignment.id\"\n                 :to=\"{name: 'teacher.assignments.show', params: { assignmentID: assignment.id }}\"\n         class=\"mb-3 max-w-sm md:max-w-xl md:w-3/4 md:mx-auto rounded rounded-xl justify-between overflow-hidden bg-gray-secondary flex flex-row px-3 pt-5 pb-3\">\n\n      <div class=\"\">\n        <assignment-score-circle\n            :display-value=\"assignment.marks\" :diameter=\"70\" :score=\"parseInt(assignment.marks)\" :total-score=\"100\"\n            start-color=\"purple\" stroke-linecap=\"butt\"\n            inner-stroke-color=\"#F1F3F6\" :stroke-width=\"6\" :inner-stroke-width=\"6\"\n        />\n      </div>\n\n      <div class=\"flex flex-col w-9/12 justify-between\">\n        <div>\n          <div class=\"text-left text-purple-primary text-xs-plus  truncate  pr-10\"> {{ assignment.title }}</div>\n\n          <div class=\"text-left text-purple-primary text-px-10  truncate mt-2 pr-10\"> {{ assignment.written_description }}</div>\n\n        </div>\n\n        <div class=\"flex flex-row w-full text-left text-px-10 text-purple-secondary justify-between\">\n          <div class=\"w-1/2 truncate mr-2\"> {{ assignment.subject_name }}</div>\n          <div class=\"w-1/2\"> {{ getHumanDate(assignment.due_datetime) }}</div>\n        </div>\n      </div>\n\n\n    </router-link>\n  </div>\n</template>\n\n<script>\nimport TeacherRepository from \"@/repositories/TeacherRepository\";\nimport StudentRepository from \"@/repositories/StudentRepository\";\nimport moment from \"moment\";\nimport CircleProgressBar from \"@/components/CircleProgressBar\";\nimport AssignmentScoreCircle from \"@/views/students/StudentDetails/components/AssignmentScoreCircle\";\nimport router from \"@/router\";\n\nexport default {\n  name: \"StudentAssignments\",\n  components: {AssignmentScoreCircle, CircleProgressBar},\n  props: {\n    userRole: String,\n    isAuthStudent:Boolean,\n    studentID:  [String, Number],\n  },\n  data() {\n    return {\n      // states\n      error: null,\n      loading: false,\n      awaitingSearch: false,\n\n      // data\n      assignments: [],\n    }\n  },\n  watch: {\n    '$route': 'fetchData',\n  },\n  methods: {\n\n    fetchData() {\n      this.error = this.assignments = null\n      this.loading = true\n\n      StudentRepository.getMarkedAssignments(this.studentID)\n          .then(response => {\n            this.assignments = response.data.data\n            this.loading = false\n\n          })\n    },\n    getHumanDate(datetime) {\n      return moment(datetime, \"YYYY-MM-DD hh:mm:ss\").format(\"DD MMMM YYYY\")\n    }\n  },\n  mounted() {\n    this.fetchData()\n  },\n  created() {\n    if (this.userRole !== 'Teacher' && !this.isAuthStudent ) {\n      router.push({name:'student.profile.show'})\n    }\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"mappings":";AAmCA,OAAOA,iBAAgB,MAAO,kCAAkC;AAChE,OAAOC,iBAAgB,MAAO,kCAAkC;AAChE,OAAOC,MAAK,MAAO,QAAQ;AAC3B,OAAOC,iBAAgB,MAAO,gCAAgC;AAC9D,OAAOC,qBAAoB,MAAO,kEAAkE;AACpG,OAAOC,MAAK,MAAO,UAAU;AAE7B,eAAe;EACbC,IAAI,EAAE,oBAAoB;EAC1BC,UAAU,EAAE;IAACH,qBAAqB;IAAED;EAAiB,CAAC;EACtDK,KAAK,EAAE;IACLC,QAAQ,EAAEC,MAAM;IAChBC,aAAa,EAACC,OAAO;IACrBC,SAAS,EAAG,CAACH,MAAM,EAAEI,MAAM;EAC7B,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACL;MACAC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE,KAAK;MACdC,cAAc,EAAE,KAAK;MAErB;MACAC,WAAW,EAAE;IACf;EACF,CAAC;EACDC,KAAK,EAAE;IACL,QAAQ,EAAE;EACZ,CAAC;EACDC,OAAO,EAAE;IAEPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACN,KAAI,GAAI,IAAI,CAACG,WAAU,GAAI,IAAG;MACnC,IAAI,CAACF,OAAM,GAAI,IAAG;MAElBhB,iBAAiB,CAACsB,oBAAoB,CAAC,IAAI,CAACV,SAAS,EAChDW,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACN,WAAU,GAAIM,QAAQ,CAACV,IAAI,CAACA,IAAG;QACpC,IAAI,CAACE,OAAM,GAAI,KAAI;MAErB,CAAC;IACP,CAAC;IACDS,YAAYA,CAACC,QAAQ,EAAE;MACrB,OAAOzB,MAAM,CAACyB,QAAQ,EAAE,qBAAqB,CAAC,CAACC,MAAM,CAAC,cAAc;IACtE;EACF,CA